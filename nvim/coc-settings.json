{
  "coc.preferences.formatOnSaveFiletypes": [
    "html",
    "ex",
    "exs",
    "heex",
    "json",
    "js",
    "jsx"
  ],
  "codeLens.enable": true,
  "languageserver": {
    "elixirLS": {
      "command": "/opt/homebrew/bin/elixir-ls",
      "filetypes": ["elixir", "eelixir"]
    }
  },
  "elixirLS.projectDir": "."
}
